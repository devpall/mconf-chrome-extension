{
  "name": "Mconf",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Access your web conference room in Mconf with a single click.",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Open your web conference room!",
    "default_popup": "login.html"
  },
  "permissions": [
    "https://mconf.org/"
  ]
}
